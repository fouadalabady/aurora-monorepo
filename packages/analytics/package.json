    "@types/react": "^18.2.45",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "next": "^14.0.0",
    "react": "^18.0.0"
  }
}