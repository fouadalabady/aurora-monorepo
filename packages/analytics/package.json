{
  "name": "@workspace/analytics",
  "version": "0.1.0",
  "description": "Aurora analytics package with Plausible Analytics",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts,.tsx",
